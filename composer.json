{
    "name": "puwnz/google-maps-serializer-bundle",
    "description": "Serialize response from GoogleMapsBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "puwnz/google-maps-bundle": "dev-develop",
        "symfony/http-kernel": "^4.0|^5.0",
        "symfony/serializer": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Puwnz\\GoogleMapsSerializerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puwnz\\GoogleMapsSerializerBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests" : [
            "php vendor/bin/phpunit --coverage-text"
        ]
    }
}
